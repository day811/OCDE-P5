# Name of the created database in Mongo, defaut = test
MONGO_DB_NAME=healthcare

# Name of the root username in Mongo, defaut = admin
# After first run, must be change first in MongoDB shell
MONGO_INITDB_ROOT_USERNAME=admin

# Name of the root user in Mongo, defaut = test
# After first run, must be change first in MongoDB shell
MONGO_INITDB_ROOT_PASSWORD=secret

# Name of the host of Mongo, defaut = mongo
MONGO_HOST=mongo

# Name of port used by Mongo, defaut = 27017
MONGO_PORT=27017

# logger uses debug level mode when True , defaut = False
MIGRATION_DEBUG=False

# Number of the first row handle by the migration
DEBUG_START=0

# Quantity of row to handle, 0= no limit, default = 0
DEBUG_LIMIT=0

# When true, no action are made to the mongo database, default = True
DEBUG_TRACE_ONLY=False
